{
	"compilerOptions": {
		"target": "es2020",
		"module": "esnext",
		"moduleResolution": "node",
		"strict": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"allowJs": true,
		"noEmit": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true
	},
	"include": ["server/**/*"]
}
